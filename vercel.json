{
  "version": 2,
  "name": "zuasoko-app",
  "framework": null,
  "buildCommand": "ls -la && pwd && node vercel-build.js",
  "outputDirectory": "frontend/dist",
  "installCommand": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
