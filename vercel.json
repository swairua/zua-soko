{
  "version": 2,
  "buildCommand": "cd frontend && npm ci && npm run build:prod",
  "outputDirectory": "frontend/dist",
  "installCommand": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
