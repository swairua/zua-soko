{
  "version": 2,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "frontend/dist",
  "installCommand": null,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "DISABLE_CACHE": "1"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
