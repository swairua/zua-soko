{
  "name": "zuasoko-app",
  "version": "1.0.0",
  "description": "Zuasoko Agricultural Platform - Full Stack Application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "npm run dev"
  },
  "keywords": [
    "agriculture",
    "marketplace",
    "react",
    "nodejs",
    "postgresql",
    "mpesa"
  ],
  "author": "Zuasoko Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "argon2": "^0.43.1"
  }
}
