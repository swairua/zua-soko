    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.1.0",
    "zustand": "^4.4.7",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pg": "^8.11.3",
