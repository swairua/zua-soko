  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production && mv dist/index.production.html dist/index.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
